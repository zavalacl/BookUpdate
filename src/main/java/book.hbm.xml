<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zavala.hibernate1.Book" table="books">
		<id name="rank" column="Rank" type="int">
			<generator class="increment"></generator>
		</id>
		
		<property name="title" column="Title" type="string"></property>
		<property name="author" column="Author" type="string"></property>
		<property name="sales" column="Sales" type="int"></property>
		<property name="imprint" column="Imprint" type="string"></property>
		<property name="publisher" column="Publisher" type="string"></property>
		<property name="yearPublished" column="YearPublished" type="int"></property>
		<property name="genre" column="Genre" type="string"></property>
		<property name="status" column="Status" type="int"></property>
		<property name="borrower" column="Borrower" type="int"></property>	
		
	</class>
</hibernate-mapping>